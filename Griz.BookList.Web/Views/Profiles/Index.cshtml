@model System.Collections.Generic.List<Griz.BookList.Lib.Models.UserProfile>

	

<table class="table table-striped">
	<tr>
		<th>Id</th>
		<th>User Name</th>
		<th>Name</th>
		<th>Admin?</th>
	</tr>
	@foreach (var profile in Model)
	{
		<tr>
			<td>@profile.Id</td>
			<td>@profile.UserName</td>
			<td>@profile.Name</td>
			<td>@profile.IsAdmin</td>
		</tr>
	}
</table>